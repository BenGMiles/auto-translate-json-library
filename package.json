    "angular",
    "ngx-translate",
    "transloco",
    "auto-translate-json",
    "google",
    "Azure",
    "DeepL",
    "OpenAI"
  ],
  "author": "codegrue",
  "repository": {
    "type": "git",
    "url": "https://github.com/topce/auto-translate-json-library"
  },
  "contributors": [
    "codegrue",
    "topce"
  ],
  "license": "MIT",

    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/node": "20.1.4",
    "@types/yargs": "17.0.24",
    "husky": "8.0.3",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-translate": "3.332.0",
    "@google-cloud/translate": "7.2.1",
    "axios": "1.4.0",
    "dotenv": "16.0.3",
    "form-data": "4.0.0",
    "openai": "3.2.1",
    "uuid": "9.0.0",
    "yargs": "17.7.2"
